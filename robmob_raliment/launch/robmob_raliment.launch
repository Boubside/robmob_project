<?xml version="1.0"?>
<launch>
  <node pkg="robmob_teleop" type="robmob_teleop_node" name="robmob_teleop_node" output="screen"/>
	<node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" args="scan:=/scan">
    <param name="xmin" value="-5.0"/>
    <param name="xmax" value="5.0"/>
    <param name="ymin" value="-5.0"/>
    <param name="ymax" value="5.0"/>
    <param name="delta" value="0.05"/>
  </node>
  <node pkg="joy" type="joy_node" name="joy_node" output="screen"/>
  <node pkg="robmob_planif" type="robmob_planif_node" name="robmob_planif_node" output="screen"/>
  <node pkg="robmob_raliment" type="robmob_raliment_node" name="robmob_raliment_node" output="screen"/>
</launch>
